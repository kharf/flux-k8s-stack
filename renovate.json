{
  "labels": ["dependencies", "renovate"],
  "extends": [
    "config:base"
  ],
  "platform": "github",
  "semanticCommits": "enabled",
  "rebaseWhen": "auto",
  "flux": {
    "fileMatch": ["\\.yaml$"]
  },
  "github-actions": {
      "fileMatch": ["^.github/(?:workflows|actions)/.+\\.cue$"]
  }
}
